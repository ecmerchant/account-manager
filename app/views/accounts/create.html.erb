<div class="page-header" id="banner">
  <div class="row">
    <div class="col-lg-10 col-md-8">
      <h2>ユーザ一登録</h2>
      <%= form_for(@account, {url: accounts_create_path, method: 'post'}) do |f| %>
        <div class="form-group">
          <%= f.label :user, "アカウントID" %>
          <%= f.text_field :user, class: 'form-control' %>
        </div>
        <div class="form-group">
          <%= f.label :password, "パスワード" %>
          <%= f.text_field :password, class: 'form-control'  %>
        </div>
        <%= f.hidden_field :admin_user, value: @master %>
        <%= f.hidden_field :isvalid, value: true %>
        <div class="actions">
          <%= f.submit "設定する", {id: "setup_btn", onClick: 'alert("更新しました")'} %>
        </div>
      <% end %>

    </div>
  </div>
</div>
